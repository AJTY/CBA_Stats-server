<h1>Listing liga_tymies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liga_tymies.each do |liga_tymy| %>
      <tr>
        <td><%= link_to 'Show', liga_tymy %></td>
        <td><%= link_to 'Edit', edit_liga_tymy_path(liga_tymy) %></td>
